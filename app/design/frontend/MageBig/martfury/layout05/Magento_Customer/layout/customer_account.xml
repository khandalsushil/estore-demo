<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Customer My Account (All Pages)" design_abstraction="custom">
    <head>
        <title>Dashboard</title>
    </head>
    <body>
        <attribute name="class" value="account"/>
        <referenceContainer name="sidebar.main">
                        
           <referenceBlock name="sidebar.main.account_nav">
              <referenceBlock name="customer_account_navigation">
                  <!-- My Account link -->
                <referenceBlock class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-link">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Dashboard</argument>
                            <argument name="path" xsi:type="string">customer/account</argument>
                            <argument name="sortOrder" xsi:type="number">250</argument>
                        </arguments>
                    </referenceBlock>
                <!-- Account Information link -->
                <referenceBlock class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-edit-link">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Account Information</argument>
                            <argument name="path" xsi:type="string">customer/account/edit</argument>
                            <argument name="sortOrder" xsi:type="number">249</argument>
                        </arguments>
                    </referenceBlock>
            <!-- Address link -->
           <referenceBlock name="customer-account-navigation-address-link">
            <arguments>
               <argument name="label" xsi:type="string" translate="true">Address Book</argument>
               <argument name="path" xsi:type="string">customer/address</argument>
               <argument name="sortOrder" xsi:type="number">248</argument>
            </arguments>
           </referenceBlock>

        <!-- My Tickets -->
        <referenceBlock name="customer-account-navigation-po-zendesk-tickets-link" remove="true"/>
        <!-- My Returns -->
        <referenceBlock name="customer-account-navigation-returns-link" remove="true"/>
        <!-- Product review link -->
        <!-- <referenceBlock name="customer-account-navigation-product-reviews-link" remove="true"/> -->
        <!-- Subscription link -->
        <referenceBlock name="customer-account-navigation-newsletter-subscriptions-link" remove="true"/>
        <!-- Billing Aggrement  link -->
        <referenceBlock name="customer-account-navigation-billing-agreements-link" remove="true"/>
        <!-- Downloadable product link -->
        <referenceBlock name="customer-account-navigation-downloadable-products-link" remove="true"/>
        <!-- My credit card link -->
        <referenceBlock name="customer-account-navigation-my-credit-cards-link" remove="true"/>
             </referenceBlock>
      </referenceBlock>
    </referenceContainer>
    </body>
</page>
